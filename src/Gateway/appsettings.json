{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "PushNotifications": {
    "ExpoApiUrl": "https://exp.host/--/api/v2/push/send"
  },
  "Observability": {
    "OtlpEndpoint": "http://localhost:4317",
    "ElasticsearchUrl": "http://localhost:9200"
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "api-route": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "api-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5000"
          }
        }
      }
    }
  }
}