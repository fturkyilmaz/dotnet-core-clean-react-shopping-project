{
  "RabbitMq": {
    "Url": "amqp://localhost:5672",
    "Username": "guest",
    "Password": "guest"
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "Postgres": {
    "ConnectionString": "Host=localhost;Port=5432;Database=ShoppingProjectDb;Username=postgres;Password=postgres"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Elasticsearch"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://localhost:9200",
          "indexFormat": "shoppingproject-logs-{0:yyyy.MM.dd}",
          "autoRegisterTemplate": true,
          "autoRegisterTemplateVersion": "ESv7"
        }
      }
    ]
  },
  "JwtSettings": {
    "Secret": "YourSuperSecretKeyHere_MustBeAtLeast32CharactersLong",
    "Issuer": "ShoppingProject",
    "Audience": "ShoppingProject",
    "ExpiryMinutes": 60,
    "RefreshTokenExpiryDays": 7
  },
  "Hangfire": {
    "ConnectionString": "Host=localhost;Database=ShoppingProjectDb;Username=postgres;Password=postgres",
    "WorkerCount": 5,
    "DashboardPath": "/hangfire"
  },
  "Authentication": {
    "ApiKey": "your-secret-api-key-here"
  },
  "AllowedHosts": "*",
  "Consul": {
    "Host": "http://localhost:8500"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=ShoppingProjectDb;Username=postgres;Password=postgres",
    "DefaultConnection_ReadOnly": "Host=localhost;Database=ShoppingProjectDb;Username=postgres;Password=postgres",
    "AuditConnection": "Host=localhost;Database=AuditDb;Username=postgres;Password=postgres",
    "RedisConnection": "localhost:6379,abortConnect=false",
    "RabbitMqConnection": "amqp://guest:guest@localhost:5672/"
  },
  "ServiceBusOption": {
    "Url": "amqp://guest:guest@localhost:5672"
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "ShoppingProject API",
        "Uri": "http://localhost:5000/health"
      }
    ],
    "EvaluationTimeInSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  },
  "AllowedOrigins": [
    "http://localhost:5173",
    "http://localhost:3000",
    "http://localhost:5000",
    "http://localhost:5050"
  ],
  "Email": {
    "SmtpHost": "localhost",
    "SmtpPort": "2525",
    "SmtpUser": "",
    "SmtpPass": "",
    "From": "pacike6451@httpsu.com",
    "EnableSsl": false,
    "ClientUrl": "http://localhost:5050"
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "QuotaExceededResponse": {
      "Content": "{{ \"message\": \"Too many requests, please try again later.\", \"details\": \"Quota exceeded. Maximum allowed: {0} per {1}. Please try again in {2} second(s).\" }}",
      "ContentType": "application/json",
      "StatusCode": 429
    },
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 100
      },
      {
        "Endpoint": "*:/api/v*/identity/login",
        "Period": "1m",
        "Limit": 5
      },
      {
        "Endpoint": "*:/api/v*/identity/register",
        "Period": "1h",
        "Limit": 10
      },
      {
        "Endpoint": "*:/api/v*/identity/refresh-token",
        "Period": "1m",
        "Limit": 10
      },
      {
        "Endpoint": "get:/api/v*/products",
        "Period": "1m",
        "Limit": 60
      },
      {
        "Endpoint": "get:/api/v*/products/*",
        "Period": "1m",
        "Limit": 120
      },
      {
        "Endpoint": "post:/api/v*/products",
        "Period": "1m",
        "Limit": 20
      },
      {
        "Endpoint": "put:/api/v*/products/*",
        "Period": "1m",
        "Limit": 20
      },
      {
        "Endpoint": "delete:/api/v*/products/*",
        "Period": "1m",
        "Limit": 20
      },
      {
        "Endpoint": "get:/api/v*/carts",
        "Period": "1m",
        "Limit": 60
      },
      {
        "Endpoint": "get:/api/v*/carts/*",
        "Period": "1m",
        "Limit": 60
      },
      {
        "Endpoint": "post:/api/v*/carts",
        "Period": "1m",
        "Limit": 30
      },
      {
        "Endpoint": "put:/api/v*/carts/*",
        "Period": "1m",
        "Limit": 30
      },
      {
        "Endpoint": "delete:/api/v*/carts/*",
        "Period": "1m",
        "Limit": 30
      },
      {
        "Endpoint": "*:/api/v*/cache/*",
        "Period": "1m",
        "Limit": 10
      }
    ]
  }
}